<?xml version="1.0"?>
<package format="3">
  <name>octocan</name>
  <version>0.0.1</version>
  <description>The octocan package</description>

  <maintainer email="jordan@example.com">Jordan</maintainer>

  <license>BSD-3-Clause</license>

  <!-- Documentation and source code repositories -->
  <!-- Example URLs:
  <url type="website">http://example.com/octocan</url>
