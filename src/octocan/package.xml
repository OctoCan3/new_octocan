  -->

  <!-- Authors are optional and can be added as needed -->
  <!-- Example author:
  <author email="contributor@example.com">Contributor Name</author>
  -->

  <!-- Build and execution dependencies -->
  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>rclcpp</depend>
  <!-- Additional dependencies, uncomment as needed:
  <depend>std_msgs</depend>
  <depend>sensor_msgs</depend>
  -->

  <!-- Build and build export dependencies, if needed:
  <build_depend>ament_cmake_python</build_depend>
  <build_export_depend>ament_cmake_python</build_export_depend>
  -->

  <!-- Test dependencies, if needed:
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>
  -->

  <!-- Documentation dependencies, if needed:
  <doc_depend>doxygen</doc_depend>
  -->

  <!-- Export tag for other unspecified tags and interfaces -->
  <export>
    <!-- metapackages, rmw implementations, or other tools can request additional information be placed here -->
  </export>
</package>
